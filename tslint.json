{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "no-unused-variable": false,
    "no-bitwise": false,
    "arrow-parens": [
      true,
      "ban-single-arg-parens"
    ],
    "one-line": [
      true,
      "check-open-brace",
      "check-whitespace"
    ]
  },
  "rulesDirectory": []
}